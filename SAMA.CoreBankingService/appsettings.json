{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "Kafka": {
        "BootstrapServers": "localhost:9092",
        "GroupId": "core-banking-group",
        "AutoOffsetReset": "Earliest",
        "EnableAutoCommit": false
    },
    "CoreBanking": {
        "BaseUrl": "https://core-banking-api.internal",
        "ApiKey": "your-api-key-here",
        "TimeoutSeconds": 30
    }
}